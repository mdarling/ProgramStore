<h1>Listing courses</h1>

<table>
  <thead>
    <tr>
      <th>Institutional entity</th>
      <th>Co or prerequisite string</th>
      <th>Co or prerequisite array</th>
      <th>Corequisite string</th>
      <th>Corequisite array</th>
      <th>Credits</th>
      <th>Description</th>
      <th>Number</th>
      <th>Prerequisite array</th>
      <th>Prerequisite string</th>
      <th>Semester offering</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.institutional_entity_id %></td>
        <td><%= course.co_or_prerequisite_string %></td>
        <td><%= course.co_or_prerequisite_array %></td>
        <td><%= course.corequisite_string %></td>
        <td><%= course.corequisite_array %></td>
        <td><%= course.credits %></td>
        <td><%= course.description %></td>
        <td><%= course.number %></td>
        <td><%= course.prerequisite_array %></td>
        <td><%= course.prerequisite_string %></td>
        <td><%= course.semester_offering %></td>
        <td><%= course.title %></td>
        <td><%= link_to 'Show', course %></td>
        <td><%= link_to 'Edit', edit_course_path(course) %></td>
        <td><%= link_to 'Destroy', course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course', new_course_path %>
